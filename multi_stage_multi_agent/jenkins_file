pipeline {
  agent none
  stages {
    stage('Back_end') {
      agent {
        docker { image 'maven:3.8.1-adoptopenjdk-11' }
      }
      steps {
        sh 'mvn --version'
      }
    }
    ############
    stage('Front_end) {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
        sh 'node --verison'
      }
    }
  }
}

    

        
